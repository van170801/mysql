<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GET CRUD</title>
    <style>
        table, th, td {
          border:1px solid black;
        }
    </style>
</head>
<body>
    <table style="width:100%">
        <tr>
          <th>username</th>
          <th>email</th>
          <th>action</th>
          
        </tr>

        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].username %></td>
              <td><%= dataTable[i].email %></td>
              <td>
                <a href="/edit-crud?id=<%= dataTable[i].id %> " type="button">Edit</a>
                <a type="button">Delete</a>
              </td>
            </tr>
         <% } %>

      </table>
</body>
</html>

